# Maximum number of active sessions allowed per user
MAX_SESSIONS=5

# Node.js environment: "development" or "production"
NODE_ENV=development

# Port number the server will run on
PORT=3000

# MongoDB connection string (replace with actual connection string in .env)
MONGODB_URI=mongodb+srv://<username>:<password>@<cluster>.mongodb.net/<database>?retryWrites=true&w=majority&appName=<app-name>

# Secrets for JWT authentication (replace with strong secrets in .env)
ACCESS_TOKEN_SECRET=your_access_token_secret
REFRESH_TOKEN_SECRET=your_refresh_token_secret
